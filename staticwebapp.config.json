{
  "navigationFallback": {
    "rewrite": "index.html",
    "exclude": [
      "/images/*.{png,jpg,gif,ico,svg}",
      "/css/*",
      "/js/*",
      "/*.{css,js}"
    ]
  },
  "routes": [
    {
      "route": "/index.html",
      "serve": "/index.html"
    },
    {
      "route": "/about.html",
      "serve": "/about.html"
    },
    {
      "route": "/faq.html",
      "serve": "/faq.html"
    },
    {
      "route": "/contact.html",
      "serve": "/contact.html"
    },
    {
      "route": "/*",
      "serve": "/404.html"
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/404.html"
    }
  }
} 